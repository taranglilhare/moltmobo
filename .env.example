# MoltMobo Environment Variables
# Copy this file to .env and fill in your values

# Anthropic Claude API Key
ANTHROPIC_API_KEY=your_api_key_here

# Optional: Ollama endpoint (if using local LLM)
OLLAMA_ENDPOINT=http://localhost:11434

# ADB Configuration
ADB_DEVICE_IP=localhost
ADB_PORT=5555

# Logging Level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Privacy Mode (true = prefer local LLM for sensitive data)
PRIVACY_MODE=true
